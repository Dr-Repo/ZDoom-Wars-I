    ETUN A 0 A_JumpIfTargetinLOS(2, 140)
    ETUN A 0 A_Jump(40, "Giveup")
    ETUN A 1 A_PlaySound("gibbage/xf2step")
    ETUN A 4 A_Chase
    ETUN B 5 A_Chase
    ETUN C 1 A_PlaySound("gibbage/xf2step")
    ETUN C 4 A_Chase
    ETUN D 5 A_Chase
	loop
  See2:
	ETUN A 0 A_UnsetReflectiveInvulnerable
    ETUN A 0 A_JumpIfTargetinLOS(2, 140)
    ETUN A 0 A_Jump(40, "Giveup")
	ETUN A 0 A_SpawnItemEx("enchantparticle", 8,-10,34,random(-3,3),random(-3,3),0,0)
    ETUN A 1 A_PlaySound("gibbage/xf2step")
    ETUN A 4 A_Chase
	ETUN A 0 A_SpawnItemEx("enchantparticle", 10,30,34,random(-3,3),random(-3,3),0,0)
    ETUN B 5 A_Chase
	ETUN A 0 A_SpawnItemEx("enchantparticle", 20,40,34,random(-3,3),random(-3,3),0,0)
