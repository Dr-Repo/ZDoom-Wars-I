    ETUN C 4 A_Chase
	ETUN C 0 A_SpawnItemEx("enchantparticle", 10,30,34,random(-3,3),random(-3,3),0,0)
    ETUN D 5 A_Chase
    loop
  Idle:
    ETUN A 0 A_JumpIfInventory("Enchantitem", 1, "Idle2")
	ETUN A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	ETUN ABCD 4 A_Wander
	ETUN A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	loop
  Idle2:
	ETUN A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	ETUN AB 4 A_Wander
	ETUN A 0 A_SpawnItemEx("enchantparticle", 20,40,34,random(-3,3),random(-3,3),0,0)
	ETUN CD 4 A_Wander
	ETUN A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	loop
  Giveup:
    ETUN A 0 A_ClearTarget
    goto See
  Melee:
    ETUN A 0 A_JumpIfInventory("Enchantitem", 1, "Melee2")
	ETUN E 5 A_FaceTarget
	ETUN F 6 A_FaceTarget
    ETUN G 4 A_CustomMeleeAttack(random(5, 8), "ettinattack")
    goto See
  Melee2:
	ETUN E 5 A_FaceTarget
	ETUN F 6 A_FaceTarget
	ETUN A 0 A_Jump(190, 2)
	EETN G 0 A_CustomMissile ("ettinmagicmelee", 40, 0,0)
    ETUN G 4 A_CustomMeleeAttack (random(5, 8), "ettinattack")
    goto See
  Pain:
    ETUN H 4 A_Pain
    ETUN H 0 A_SetReflectiveInvulnerable
    ETUN H 25 A_CentaurDefend
    ETUN H 0 A_UnsetReflectiveInvulnerable
    goto See
  Death:
  	ETUN I 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	ETUN I 0 A_GiveToTarget("GreenMana",1)
	ETUN I 0 A_GiveToTarget("GreenHeroMana",1)
    ETUN IJ 4
    ETUN K 4 A_Scream
    ETUN L 4 A_NoBlocking
    ETUN MN 4
    ETUN O 4 A_PlaySound("gibbage/xbody")
    ETUN P 4
