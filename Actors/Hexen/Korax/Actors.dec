    goto See
  Death:
	KORX A 0 ACS_NamedExecuteAlways("ManaReimbursement",0,5,1)
  	KORX A 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	KORX A 0 A_GiveToTarget("RedMana",1)
	KORX A 0 A_GiveToTarget("RedHeroMana",1)
