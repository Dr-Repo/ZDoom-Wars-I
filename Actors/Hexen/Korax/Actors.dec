    KORX H 5 A_FaceTarget
    KORX H 5 A_Scream
    KORX IIIIII 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX I 1 A_CustomMissile ("GREENBOSSGIBBER", 65, 0, 16, 2, 48)
    KORX IIIIIIIIIIIIIIIIII 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX I 1 A_CustomMissile ("GREENBOSSGIBBER", 60, 0, 116, 2, 32)
    KORX IIIIIIIIIIIIIIIIIIII 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX I 1 A_CustomMissile ("GREENBOSSGIBBER", 60, 0, 80, 2, 66)
    KORX IIIIIIIIIIIIIIIIIIIIIII 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX JK 5 
    KORX LMN 5
    KORX O 5 A_PlaySound("gibbage/bonethud")
    KORX P 5
    KORX Q 10 A_PlaySound("gibbage/xbody")
    KORX R 1 A_PlaySound ("gibbage/bonethud")
    KORX R 1 A_SpawnItemEx ("ZKoraxSpirit", 0, 0, 32, 8, 0, 0, 60, 128, 0)
    KORX R 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX R 1 A_SpawnItemEx ("ZKoraxSpirit", 0, 0, 32, 8, 0, 0, 120, 128, 0)
    KORX R 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX R 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX R 1 A_SpawnItemEx ("ZKoraxSpirit", 0, 0, 32, 8, 0, 0, 180, 128, 0)
    KORX R 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX R 1 A_SpawnItemEx ("ZKoraxSpirit", 0, 0, 32, 8, 0, 0, 240, 128, 0)
    KORX R 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX R 1 A_SpawnItemEx ("ZKoraxSpirit", 0, 0, 32, 8, 0, 0, 300, 128, 0)
    KORX R 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX R 1 A_SpawnItemEx ("ZKoraxSpirit", 0, 0, 32, 8, 0, 0, 360, 128, 0)
    KORX R 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
    KORX S 5 A_NoBlocking
    KORX TU 5
    KORX V 300
	KORX V 3 A_FadeOut(0.1)
	wait
  Pain.HexenTeleporterVanilla:
	KORX A 0 A_GiveInventory("TeleportCount",1)
	KORX A 0 HealThing(1)
	KORX A 0 A_JumpIfInventory("TeleportCount", ACS_ExecuteWithResult(118,0,4,0), "TeleportCooldown")
	KORX A 0 Teleport(2012,0,0)
	goto TeleportCooldown
  TeleportCooldown:
    KORX AAABBBB 3 A_Chase("","")
    KORX C 3 A_PlaySound("gibbage/xbstep")
    KORX CCCDDDD 3 A_Chase("","")
    KORX A 3 A_PlaySound("gibbage/xbstep")
	Goto See
  Pain.HexenRemoverVanilla:
	KORX A 0 A_Die("HexenRemoverVanilla")
	Stop
  Death.HexenRemoverVanilla:
	KORX A 0 A_UnsetSolid
	KORX A 0 ACS_NamedExecuteAlways("HeroNotifications", 0, 40, 7)
	KORX A 0 ACS_NamedExecuteAlways("ManaReimbursement",0,5,1)
	KORX A 0 A_SpawnItemEx("HexenTelefog")
	KORX A 0 A_SpawnItem ("RedMana")
	KORX A 0 A_SpawnItem ("RedHeroMana")
	Stop
  }
}

