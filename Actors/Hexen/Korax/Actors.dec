//Vanilla Korax
ACTOR ZKorax : Korax replaces Korax
{
  Health 6000
  Mass 5250
  Height 110
  Radius 55
  Speed 10
  Minmissilechance 60
  Painchance 0
  Painchance "HexenRemoverVanilla", 255
  Painchance "HexenTeleporterVanilla", 255
  SeeSound "KoraxSight"
  AttackSound "KoraxAttack"
  PainSound "KoraxPain"
  DeathSound "KoraxDeath"
  ActiveSound "KoraxActive"
  Damagefactor "Annihilate", 0
  Damagefactor "Ridiculous", 0.6
  MONSTER
  +NOICEDEATH
  +NODROPOFF
  +DONTRIP
  -COUNTKILL
  +NORADIUSDMG
  +QUICKTORETALIATE
  -TELESTOMP
  -NOBLOOD
  -BOSS
  -DROPOFF
  Dropitem "RedMana", 256
  Dropitem "RedHeroMana", 256
  Dropitem "ZCrystalVial", 256
  Dropitem "ZCrystalVial", 256
  Dropitem "ZCrystalVial", 256
  Obituary "%o was swept from the board by Korax."
  states
  {
  Spawn:
    KORX A 0
	KORX A 0 A_RearrangePointers(AAPTR_DEFAULT,AAPTR_DEFAULT,AAPTR_TARGET)
    KORX A 0 ACS_NamedExecuteAlways("HeroNotifications", 0, 15, 1)
	KORX A 0 ACS_NamedExecuteAlways("HealthBar", 0, 115)
    KORX AB 5 A_Look
    goto See
  Giveup:
    KORX A 0 A_ClearTarget
    goto See
  See:
	KORX A 0
    KORX AAABBBB 3 A_Chase
    KORX C 3 A_PlaySound("gibbage/xbstep")
    KORX CCCDDDD 3 A_Chase
    KORX A 3 A_PlaySound("gibbage/xbstep")
	KORX A 0 A_JumpIfTargetinLOS(2, 140)
    KORX A 0 A_Jump(30, "Giveup")
    loop
  Idle:
    KORX A 0 A_LookEx(0, 0, 0, 0, 360, "See")
    KORX AAABBBB 3 A_Wander
	KORX A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	KORX CCCDDDD 3 A_Wander
	KORX A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	loop
  Pain:
    KORX H 5 A_Pain
    KORX H 5
    goto See
  Missile:
    KORX E 6 Bright A_FaceTarget
    KORX E 0 A_PlaySound("KoraxAttack")
    KORX E 0 A_Jump(256, "Wraith", "DemonFX1", "DemonFX2", "Firedemon", "Centaur", "Serpent")
    goto See
  Serpent:
	KORX A 0 A_CustomMissile ("ZSerpentFX", 55, -52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZSerpentFX", 55, 52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZSerpentFX", 82, -52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZSerpentFX", 82, 52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZSerpentFX", 110, -35, 0, 4, 0)
	KORX F 6 A_CustomMissile ("ZSerpentFX", 110, 35, 0, 4, 0)
	KORX E 4 Bright
    goto See
  Wraith:
	KORX A 0 A_CustomMissile ("kZWraithFX1", 55, -52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("kZWraithFX1", 55, 52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("kZWraithFX1", 82, -52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("kZWraithFX1", 82, 52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("kZWraithFX1", 110, -35, 0, 4, 0)
	KORX F 6 A_CustomMissile ("kZWraithFX1", 110, 35, 0, 4, 0)
	KORX E 4 Bright
    goto See
  DemonFX1:
	KORX A 0 A_CustomMissile ("ZDemon1FX12", 55, -52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZDemon1FX12", 55, 52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZDemon1FX12", 82, -52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZDemon1FX12", 82, 52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZDemon1FX12", 110, -35, 0, 4, 0)
	KORX F 6 A_CustomMissile ("ZDemon1FX12", 110, 35, 0, 4, 0)
	KORX E 4 Bright
    goto See
  DemonFX2:
	KORX A 0 A_CustomMissile ("ZDemon2FX12", 55, -52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZDemon2FX12", 55, 52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZDemon2FX12", 82, -52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZDemon2FX12", 82, 52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZDemon2FX12", 110, -35, 0, 4, 0)
	KORX F 6 A_CustomMissile ("ZDemon2FX12", 110, 35, 0, 4, 0)
	KORX E 4 Bright
    goto See
  FireDemon:
	KORX A 0 A_CustomMissile ("ZFiredemonmissile2", 55, -52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZFiredemonmissile2", 55, 52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZFiredemonmissile2", 82, -52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZFiredemonmissile2", 82, 52, 0, 4, 0)
	KORX A 0 A_CustomMissile ("ZFiredemonmissile2", 110, -35, 0, 4, 0)
	KORX F 6 A_CustomMissile ("ZFiredemonmissile2", 110, 35, 0, 4, 0)
	KORX E 4 Bright
    goto See
  Centaur:
	KORX A 0 A_CustomMissile ("kZCentaurFX", 55, -52, 0, 4, 0)
