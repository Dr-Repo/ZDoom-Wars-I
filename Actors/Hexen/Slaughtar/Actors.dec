//Vanilla Slaughtar
ACTOR ZCentaurLeader : CentaurLeader replaces CentaurLeader
{
  Health 400
  Mass 600
  Speed 15
  Meleerange 74
  Scale 1.20
  DamageFactor "Electric", 1.0
  Painchance 110
  Painchance "heal", 255
  Painchance "Revived", 255
  Painchance "DOOMConvert", 255
  Painchance "HERETICConvert", 255
  Painchance "HEXENConvert", 255
  Painchance "STRIFEConvert", 255
  Painchance "CHEXConvert", 255
  Painchance "VIRUSConvert", 255
  Painchance "REDConvert", 255
  Painchance "BLUEConvert", 255
  Painchance "GREENConvert", 255
  Painchance "YELLOWConvert", 255
  Painchance "SILVERConvert", 255  
  Painchance "HexenRemoverVanilla", 255
  Painchance "HexenTeleporterVanilla", 255
  Translation "94:102=226:230", "80:93=226:227", "103:111=160:166", "3:3=162:162", "5:8=167:167"
  +PUSHABLE
  +QUICKTORETALIATE
  -COUNTKILL
  +MISSILEMORE
  -TELESTOMP
  -DROPOFF
  Bloodtype hblood
  Dropitem "PinkMana", 256
  Dropitem "PinkHeroMana", 256
  Dropitem "ZCrystalVial", 16
  States
  {
  Spawn:
    CENT AB 4 A_Look
    goto See
  See:
	CENT A 0 A_UnsetReflectiveInvulnerable
    CENT A 0 A_JumpIfTargetinLOS(2, 140)
    CENT A 0 A_Jump(40, "Giveup")
    CENT A 1 A_PlaySound("gibbage/xfstep")
    CENT A 3 A_Chase
    CENT B 4 A_Chase
    CENT C 1 A_PlaySound("gibbage/xfstep")
    CENT C 3 A_Chase
    CENT D 4 A_Chase
    loop
  Idle:
    CENT A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	CENT ABCD 4 A_Wander
	CENT A 0  A_LookEx(0, 0, 0, 0, 360, "See")
	loop
  Giveup:
    CENT A 0 A_ClearTarget
    goto See
  Melee:
    CENT HI 5 A_FaceTarget
    CENT J 6 A_CustomMeleeAttack(random(18, 22), "CentaurAttack", "CentaurAttack")
    goto See
  Missile:
    CENT E 10 A_FaceTarget
    CENT F 8 Bright A_CustomMissile("ZCentaurFX", 45, 0, 0, CMF_AIMOFFSET)
    CENT E 10 A_FaceTarget
    CENT F 8 Bright A_CustomMissile("ZCentaurFX", 45, 0, 0, CMF_AIMOFFSET)
    Goto See
  Pain.Heal:
	CENT A 0 A_GiveInventory("enchantitem", 1)
    CENT A 0 HealThing (1)
    goto See
  Death:
  	CENT K 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	CENT K 0 A_GiveToTarget("PinkMana",1)
	CENT K 0 A_GiveToTarget("PinkHeroMana",1)
    CENT K 4
    CENT L 4 A_Scream
    CENT MN 4
    CENT O 4 A_NoBlocking
    CENT P 4
    CENT Q 4 A_PlaySound("gibbage/xbody")
    CENT RS 4
	CENT T 0 A_JumpIfInventory("Revived", 1, "Death2")
    CENT T -1
    Stop
  XDeath:
	CTXD A 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	CTXD A 0 A_GiveToTarget("PinkMana",1)
	CTXD A 0 A_GiveToTarget("PinkHeroMana",1)
    CTXD A 4 A_SpawnItem("GIBBER")
    CTXD B 4 A_NoBlocking
    CTXD C 1 A_SpawnItemEx("CentaurSword", 0, 0, 45, 1 + random[CentaurDrop](-128,127)*0.08525, 1 + random[CentaurDrop](-128,127)*0.08525, 8 + random[CentaurDrop](0,255)*0.025625, 160)
    CTXD C 3 A_SpawnItemEx("CentaurShield", 0, 0, 45, 1 + random[CentaurDrop](-128,127)*0.03125, 1 + random[CentaurDrop](-128,127)*0.03125, 8 + random[CentaurDrop](0,255)*0.015625, 160)
    CTXD D 3 A_Scream
    CTXD E 4 
    CTXD F 3
    CTXD G 4 
    CTXD H 3 A_PlaySound("gibbage/xvile")
    CTXD I 4 
    CTXD J 3 
	CTXD K 0 A_JumpIfInventory("Revived", 1, "Death3")
    CTXD K 30 A_SpawnItem("GIBBER")
	Stop
  Raise:
    CENT TSRQPONMLK 4
	CENT A 10
    goto See
  Death2:
	CENT T 3 A_FadeOut(0.1)
    loop
  Death3:
	CTXD K 3 A_FadeOut(0.1)
    loop
  Pain.Revived:
	CENT A 0 A_GiveInventory("Revived", 1)
	goto See
  Pain.DOOMConvert:
	CENT A 0 A_Die("DOOMConvert")
	Stop
  Death.DOOMConvert:
    CENT A 0 A_UnsetSolid
	CENT A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.DoomConvert2")
    CENT A 0 A_SpawnItemEx("DoomZCentaurLeader",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	CENT A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.DoomConvert2:
	CENT A 0 A_UnsetSolid
    CENT A 0 A_SpawnItemEx("DoomZCentaurLeader")
	Stop
  Pain.HereticConvert:
	CENT A 0 A_Die("HereticConvert")
	Stop
  Death.HereticConvert:
    CENT A 0 A_UnsetSolid
	CENT A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.HereticConvert2")
    CENT A 0 A_SpawnItemEx("HereticZCentaurLeader",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
