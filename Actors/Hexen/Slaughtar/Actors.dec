	CENT A 0 A_UnsetSolid
    CENT A 0 A_SpawnItemEx("HereticZCentaurLeader")
	Stop
  Pain.HexenConvert:
	CENT A 0 A_Die("HexenConvert")
	Stop
  Death.HexenConvert:
    CENT A 0 A_UnsetSolid
	CENT A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.HexenConvert2")
    CENT A 0 A_SpawnItemEx("ZCentaurLeader",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	CENT A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.HexenConvert2:
	CENT A 0 A_UnsetSolid
    CENT A 0 A_SpawnItemEx("ZCentaurLeader")
	Stop
  Pain.StrifeConvert:
	CENT A 0 A_Die("StrifeConvert")
	Stop
  Death.StrifeConvert:
    CENT A 0 A_UnsetSolid
	CENT A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.StrifeConvert2")
