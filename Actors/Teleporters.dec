ACTOR DoomTeleporterSummoner : Shotgun
{
   +AMMO_OPTIONAL
   +WEAPON.NOAUTOAIM
   +WEAPON.NO_AUTO_SWITCH
   Weapon.AmmoGive 0
   Weapon.AmmoUse 0
   Weapon.AmmoType "Mana"
   Weapon.AmmoType2 "Heromana"
   Obituary "%o fell to %k's forces."
   Tag "Doom Teleporter Summoner"
   States
   {
   Spawn:
	TNT1 A 0
	loop
   Select:
	DTEL A 1 A_Raise
	loop
   Ready:
	DTEL A 0 ACS_NamedExecuteAlways("MonsterMessages", 0, 1, 25)
	goto Set
   Set:
    DTEL A 0 A_JumpIfInventory("MonsterRemover", 1, "UnitRemover") 
	DTEL A 1 A_WeaponReady
	loop
   Deselect:
	DTEL A 0 A_TakeInventory("DoomTeleporterSummoner",1)
	DTEL A 1 A_Lower
	loop
   Fire:
	/*PUNG A 0 A_JumpIfInventory ("Mana", 15, 2)
	PUNG A 3 ACS_NamedExecuteAlways("MonsterMessages", 0, 100, 0)
	goto Set
	PUNG A 6
	PUNG BC 2
	PUNG D 2 A_PlaysoundEx("Summon", "Weapon")
	PUNG D 0 A_TakeInventory("Mana", 15)
	goto Summon*/
   Summon:
	PUNG D 0 A_JumpIfInventory ("VanillaItem", 1, "VanillaSummon")
	PUNG D 0 A_JumpIfInventory ("BlueItem", 1, "BlueSummon")
	PUNG D 0 A_JumpIfInventory ("GreenItem", 1, "GreenSummon")
	PUNG D 0 A_JumpIfInventory ("RedItem", 1, "RedSummon")
	PUNG D 0 A_JumpIfInventory ("SilverItem", 1, "SilverSummon")
	PUNG D 0 A_JumpIfInventory ("YellowItem", 1, "YellowSummon")
	PUNG D 0 A_JumpIfInventory ("DRItem", 1, "DRSummon")
	PUNG D 0 A_JumpIfInventory ("DBItem", 1, "DBSummon")
	PUNG D 0 A_JumpIfInventory ("DGItem", 1, "DGSummon")
	PUNG D 0 A_JumpIfInventory ("DSItem", 1, "DSSummon")
	goto VanillaSummon
   VanillaSummon:
	PUNG D 4 A_FireCustomMissile("DoomTeleporterProjectile", 0, 1, 0, 8, 0)
	PUNG CB 2
	PUNG B 0 A_ReFire("VanillaSummonRanged")
	goto Set
   VanillaSummonRanged:
	PUNG D 4 A_FireCustomMissile("DoomTeleporterProjectile2", 0, 1, 0, 8, 0)
	PUNG CB 2
	goto Set
   BlueSummon:
	PUNG D 4 A_FireCustomMissile("BlueDoomTeleporterProjectile", 0, 1, 0, 8, 0)
	PUNG CB 2
	PUNG B 0 A_ReFire("BlueSummonRanged")
	goto Set
   BlueSummonRanged:
	PUNG D 4 A_FireCustomMissile("BlueDoomTeleporterProjectile2", 0, 1, 0, 8, 0)
	PUNG CB 2
	goto Set
   GreenSummon:
	PUNG D 4 A_FireCustomMissile("GreenDoomTeleporterProjectile", 0, 1, 0, 8, 0)
	PUNG CB 2
	PUNG B 0 A_ReFire("GreenSummonRanged")
	goto Set
   GreenSummonRanged:
	PUNG D 4 A_FireCustomMissile("GreenDoomTeleporterProjectile2", 0, 1, 0, 8, 0)
	PUNG CB 2
	goto Set
   RedSummon:
	PUNG D 4 A_FireCustomMissile("RedDoomTeleporterProjectile", 0, 1, 0, 8, 0)
	PUNG CB 2
	PUNG B 0 A_ReFire("RedSummonRanged")
	goto Set
   RedSummonRanged:
	PUNG D 4 A_FireCustomMissile("RedDoomTeleporterProjectile2", 0, 1, 0, 8, 0)
	PUNG CB 2
	goto Set
   SilverSummon:
	PUNG D 4 A_FireCustomMissile("SilverDoomTeleporterProjectile", 0, 1, 0, 8, 0)
	PUNG CB 2
	PUNG B 0 A_ReFire("SilverSummonRanged")
