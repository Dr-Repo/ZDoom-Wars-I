	PUNG CB 2
	goto Set
   AltFire:
	PUNG D 0 A_JumpIfInventory ("VanillaItem", 1, "VanillaSummon2")
	PUNG D 0 A_JumpIfInventory ("BlueItem", 1, "BlueSummon2")
	PUNG D 0 A_JumpIfInventory ("GreenItem", 1, "GreenSummon2")
	PUNG D 0 A_JumpIfInventory ("RedItem", 1, "RedSummon2")
	PUNG D 0 A_JumpIfInventory ("SilverItem", 1, "SilverSummon2")
	PUNG D 0 A_JumpIfInventory ("YellowItem", 1, "YellowSummon2")
	PUNG D 0 A_JumpIfInventory ("DRItem", 1, "DRSummon2")
