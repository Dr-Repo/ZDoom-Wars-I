	PUNG CB 2
	goto Set
   YellowSummon2:
	PUNG D 0 A_JumpIf(ACS_ExecuteWithResult(118,5,5,0)>=ACS_ExecuteWithResult(118,0,6), "Set")
	PUNG D 4 A_FireCustomMissile("YellowDoomTelepointSummonball", 0, 1, 0, 8, 0)
	PUNG CB 2
	goto Set
   DRSummon2:
	PUNG D 0 A_JumpIf(ACS_ExecuteWithResult(118,36,5,0)>=ACS_ExecuteWithResult(118,0,6), "Set")
	PUNG D 4 A_FireCustomMissile("RedTeamTelepointSummonball", 0, 1, 0, 8, 0)
