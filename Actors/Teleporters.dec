ACTOR DoomTelepoint 
{
  Mass 10000
  +SHOOTABLE
  +DONTGIB
  +NOBLOOD
  MONSTER
  //+NOCLIP 
  Health 200
  Height 16
  Radius 16
  states
  {
  Spawn:
	DTEL B 0
	DTEL B 0 BRIGHT A_RearrangePointers(AAPTR_DEFAULT, AAPTR_TARGET, AAPTR_DEFAULT)
	DTEL B 1 BRIGHT A_PlaySound("TeleporterSpawned")
	DTEL B 1 BRIGHT A_SpawnItemEx("DoomTelepoint2",0,0,48,0,0,0,0,SXF_ABSOLUTEVELOCITY | SXF_CLEARCALLERTID | SXF_TRANSFERPOINTERS)
	DTEL B 7 BRIGHT Thing_ChangeTID(0,2000)
	DTEL B 1 BRIGHT ACS_NamedExecuteAlways("Teleporter",0)
	goto See
  See:
	DTEL B 10 BRIGHT 
	Loop
  Death:	
