//Vanilla Undead Ghost Knight
ACTOR ZKnightGhost : KnightGhost replaces KnightGhost
{
  Health 400
  Mass 250
  Meleerange 84
  +PUSHABLE
  +QUICKTORETALIATE
  -NOBLOOD
  -COUNTKILL
  -DROPOFF
  -SHADOW
  Painchance "DOOMConvert", 255
  Painchance "HERETICConvert", 255
  Painchance "HEXENConvert", 255
  Painchance "STRIFEConvert", 255
  Painchance "CHEXConvert", 255
  Painchance "VIRUSConvert", 255
  Painchance "REDConvert", 255
  Painchance "BLUEConvert", 255
  Painchance "GREENConvert", 255
  Painchance "YELLOWConvert", 255
  Painchance "SILVERConvert", 255 
  Painchance "Revived", 255
  Painchance "HereticRemoverVanilla", 255
  Painchance "HereticTeleporterVanilla", 255
  Dropitem "PinkMana", 256
  Dropitem "PinkHeroMana", 256
  Dropitem "ZArtiInvisibility", 64
  Bloodtype bulletpuffed
  HitObituary "%o was slain by an Undead Warrior."
  Translation "112:127=176:191" //To make the axes red
  states
  {
  Spawn:
    KNIG AB 10 A_Look
    goto See
  See:
    KNIG A 0 A_JumpIfTargetinLOS(2, 140)
    KNIG A 0 A_Jump(30, "Giveup")
    KNIG A 0 A_PlaySound("gibbage/xbonestep")
    KNIG AB 4 A_Chase
    KNIG C 0 A_PlaySound("gibbage/xbonestep")
    KNIG CD 4 A_Chase
    loop
  Idle:
    KNIG A 0 A_LookEx(0, 0, 0, 0, 360, "See")
    KNIG ABCD 4 A_Wander
	KNIG A 0 A_LookEx(0, 0, 0, 0, 360, "See")
    loop
  Giveup:
    KNIG A 0 A_ClearTarget
    goto See
  Pain:
    KNIG H 3
    KNIG H 3 A_Pain
    Goto See
  Melee:
    KNIG E 3 A_FaceTarget
    KNIG E 2 A_FaceTarget
    KNIG F 4 A_FaceTarget
    KNIG G 4 A_CustomMeleeAttack (10, "hknight/melee")
    Goto See
  Missile:
    KNIG E 6 A_FaceTarget
    KNIG F 5 A_FaceTarget
    KNIG G 5 A_CustomMissile("ZRedAxe", 52,0,0)
    KNIG E 6 A_FaceTarget
    KNIG F 5 A_FaceTarget
    KNIG G 5 A_CustomMissile("ZRedAxe", 52,0,0)
    Goto See
  Death:
  	KNIG I 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	KNIG I 0 A_GiveToTarget("PinkMana",1)
	KNIG I 0 A_GiveToTarget("PinkHeroMana",1)
    KNIG I 6 A_SpawnItemEx("KaGas", 0, 0, random(24,42), 0, 0, random(2,6), random(0,360), 128)
    KNIG J 6 A_Scream
    KNIG K 6
    KNIG L 6 A_NoBlocking
    KNIG M 6
    KNIG N 6 A_PlaySound("gibbage/bonethud")
	KNIG O 0 A_JumpIfInventory("Revived", 1, "Death2")
    KNIG O -1
    Stop
  Raise:
    KNIG ONMLKJI 6
	KNIG A 10
    Goto See
  Death2:
	KNIG O 3 A_FadeOut(0.1)
    loop
  Pain.Revived:
	KNIG A 0 A_GiveInventory("Revived", 1)
	goto See
  Pain.DOOMConvert:
	KNIG A 0 A_Die("DOOMConvert")
	Stop
  Death.DOOMConvert:
    KNIG A 0 A_UnsetSolid
	KNIG A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.DoomConvert2")
    KNIG A 0 A_SpawnItemEx("DoomZKnightGhost",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	KNIG A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.DoomConvert2:
	KNIG A 0 A_UnsetSolid
    KNIG A 0 A_SpawnItemEx("DoomZKnightGhost")
	Stop
  Pain.HereticConvert:
	KNIG A 0 A_Die("HereticConvert")
	Stop
  Death.HereticConvert:
    KNIG A 0 A_UnsetSolid
	KNIG A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.HereticConvert2")
    KNIG A 0 A_SpawnItemEx("ZKnightGhost",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	KNIG A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.HereticConvert2:
	KNIG A 0 A_UnsetSolid
    KNIG A 0 A_SpawnItemEx("ZKnightGhost")
	Stop
  Pain.HexenConvert:
	KNIG A 0 A_Die("HexenConvert")
	Stop
  Death.HexenConvert:
    KNIG A 0 A_UnsetSolid
	KNIG A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.HexenConvert2")
    KNIG A 0 A_SpawnItemEx("HexenZKnightGhost",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	KNIG A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.HexenConvert2:
	KNIG A 0 A_UnsetSolid
    KNIG A 0 A_SpawnItemEx("HexenZKnightGhost")
	Stop
  Pain.StrifeConvert:
	KNIG A 0 A_Die("StrifeConvert")
	Stop
  Death.StrifeConvert:
    KNIG A 0 A_UnsetSolid
	KNIG A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.StrifeConvert2")
    KNIG A 0 A_SpawnItemEx("StrifeZKnightGhost",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	KNIG A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.StrifeConvert2:
	KNIG A 0 A_UnsetSolid
    KNIG A 0 A_SpawnItemEx("StrifeZKnightGhost")
	Stop
  Pain.ChexConvert:
	KNIG A 0 A_Die("ChexConvert")
	Stop
  Death.ChexConvert:
    KNIG A 0 A_UnsetSolid
	KNIG A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.ChexConvert2")
    KNIG A 0 A_SpawnItemEx("ChexZKnightGhost",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
