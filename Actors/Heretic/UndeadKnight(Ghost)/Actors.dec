	Stop
  Death.ChexConvert2:
	KNIG A 0 A_UnsetSolid
    KNIG A 0 A_SpawnItemEx("ChexZKnightGhost")
	Stop
  Pain.VirusConvert:
	KNIG A 0 A_Die("VirusConvert")
	Stop
  Death.VirusConvert:
    KNIG A 0 A_UnsetSolid
	KNIG A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.VirusConvert2")
    KNIG A 0 A_SpawnItemEx("VirusZKnightGhost",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	KNIG A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.VirusConvert2:
	KNIG A 0 A_UnsetSolid
    KNIG A 0 A_SpawnItemEx("VirusZKnightGhost")
	Stop
  Pain.BlueConvert:
	KNIG A 0 A_Die("BlueConvert")
	Stop
  Death.BlueConvert:
    KNIG A 0 A_UnsetSolid
	KNIG A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.BlueConvert2")
    KNIG A 0 A_SpawnItemEx("BlueZKnightGhost",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	KNIG A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.BlueConvert2:
	KNIG A 0 A_UnsetSolid
    KNIG A 0 A_SpawnItemEx("BlueZKnightGhost")
	Stop
  Pain.GreenConvert:
	KNIG A 0 A_Die("GreenConvert")
