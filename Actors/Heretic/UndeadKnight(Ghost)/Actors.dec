  Death.GreenConvert:
    KNIG A 0 A_UnsetSolid
	KNIG A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.GreenConvert2")
    KNIG A 0 A_SpawnItemEx("GreenZKnightGhost",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	KNIG A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.GreenConvert2:
