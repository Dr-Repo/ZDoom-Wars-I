  Painchance "REDConvert", 255
  Painchance "BLUEConvert", 255
  Painchance "GREENConvert", 255
  Painchance "YELLOWConvert", 255
  Painchance "SILVERConvert", 255
  Painchance "Revived", 255
  Painchance "ChexRemoverVanilla", 255
  Painchance "ChexTeleporterVanilla", 255
  Monster
  +FLOORCLIP
  +QUICKTORETALIATE
  +PUSHABLE
  -DROPOFF
  -COUNTKILL
  Bloodcolor "green"
  Bloodtype "GreenBlood"
  Dropitem "PinkMana", 256
  Dropitem "PinkHeroMana", 256
  Dropitem "SlimeCanister", 64  
  Obituary "%o was boogered by a stridicus."
  States
  {
  Spawn:
	STRD AB 10 A_Look
	goto See
  See:
    STRD A 0 A_JumpIfTargetinLOS(2, 200)
    STRD A 0 A_Jump(15, "Giveup")
	STRD A 1 A_PlaySound("gibbage/sstep")
	STRD A 1
	STRD AABBB 2 A_Chase
	STRD C 1 A_PlaySound("gibbage/sstep")
	STRD C 1
	STRD CCDDD 2 A_Chase
	Loop
  Idle:
    STRD A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	STRD AAABBBCCCDDD 2 A_Wander
	STRD A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	loop
  Giveup:
    STRD A 0 A_ClearTarget
    goto See
  Melee:
	STRD E 6 A_FaceTarget
	STRD F 2 A_CustomMeleeAttack (random(20, 24), "q3demon/melee")
	STRD E 0 A_Jump (245, 2)
	STRD F 2 A_CustomMissile("FlemsporeSummonball",0,0,0)
	STRD G 10
	Goto See
  Pain:
	STRD H 2
	STRD H 2 A_Pain
	Goto See
  Death:
  	STRD I 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	STRD I 0 A_GiveToTarget("PinkMana",1)
