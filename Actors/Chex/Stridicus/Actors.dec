	STRD A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.DoomConvert2")
    STRD A 0 A_SpawnItemEx("DoomStridicus",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	STRD A 0 A_DamageChildren(1, "Revived")
