	STRD M 4 A_NoBlocking
	STRD M 0 A_JumpIfInventory("Revived", 1, "Death2")
	CHST A -1
	Stop
  Raise:
	STRD NMLKJI 5
	STRD A 10
	Goto See
  Death2:
	TNT1 A 0
    stop
  Pain.Revived:
	STRD A 0 A_GiveInventory("Revived", 1)
	goto See
  Pain.DOOMConvert:
	STRD A 0 A_Die("DOOMConvert")
