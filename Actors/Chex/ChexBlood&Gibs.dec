ACTOR NashGore_DoomFlyingBlood
{
Translation "168:191=210:223",
	"16:47=210:223",
	"208:235=210:223",
	"210:223=210:223",
	"48:79=210:223"
 Scale 0.75
 Health 1
 Radius 8
 Height 1
 Mass 1
 -SOLID
 +NOCLIP
 +CANNOTPUSH
 +CORPSE
 +NOTELEPORT
 +NOBLOCKMAP
 +CLIENTSIDEONLY
 states
 {
 Spawn:
  TNT1 A 0 A_CheckSight("Cheap")
  FBLD A 1 A_SpawnItemEx("NashGore_DoomFlyingBloodTrail",0,0,0,0,0,0,0,128)
  FBLD A 1 A_JumpIf(waterlevel > 1, "Remove")
  loop
 Cheap:
  FBLD A 8 A_SpawnItemEx("NashGore_DoomFlyingBloodTrail",0,0,0,0,0,0,0,128)
  FBLD A 1 A_JumpIf(waterlevel > 1, "Remove")
  loop
 Crash:
  TNT1 A 1 A_SpawnItemEx("NashGore_DoomBloodSpot",0,0,0,1)
  stop
 Remove: 
  TNT1 A 0 
  stop
 }
}

