// Doom Gibs
ACTOR GIB1Doom
{
Translation "168:191=210:223",
	"16:47=210:223",
	"208:235=210:223",
	"210:223=210:223",
	"48:79=210:223"
    +DoomBOUNCE
    PROJECTILE
    -NOGRAVITY
    +CANNOTPUSH
    +NOBLOCKMAP
    -NOTELEPORT
    +CLIENTSIDEONLY
    -SOLID
    Radius 2
    Damage 0
    Bouncefactor 0.4
    SeeSound "gibbage/xgib"
    Speed 5
    States
    {
    Spawn:
        GIB1 A 0
        GIB1 A 0 A_Jump(64,"Alt") 
        GIB1 A 0 ThrustThingZ (0, 45, 0, 1)
        goto See
    Alt:
    	GIB1 A 0 A_SpawnItemEx ("GIB1DoomSPLAT", 1, 1, random(8,32), random(0,5), random(0,5), random(8,18), random(0,360), 128, 0)
    	Stop 
    See:
		GIB1 A 1 A_JumpIf(waterlevel > 1, "Death")
        GIB1 A 5 A_SpawnItemEx("DoomBlood", 0,0,0,0,0,0,0,128)
        GIB1 B 5 ThrustThingZ(0,6,1,1) 
        GIB1 AB 5
        loop
    Death:
        GIB1 A 0 A_Jump (64,2)
        GIB1 A 0 A_Jump (128,2)
        GIB1 A 70
        Stop
        GIB1 B 70
        Stop
    }
}

ACTOR Gib1DoomSplat : Gib1Doom
{
Translation "168:191=210:223",
	"16:47=210:223",
	"208:235=210:223",
	"210:223=210:223",
	"48:79=210:223"
	-DoomBOUNCE
    +CLIENTSIDEONLY
    States
    {
    Spawn:
        GIB1 A 0
        goto See
    Death:
		GIB1 A 0 A_JumpIf(waterlevel > 1, 8)
        SPLT A 0 A_SpawnItemEx("DoomBlood",0,0,0,0,0,0,0,128)
        SPLT A 0 A_SpawnItemEx("DoomBlood",0,0,0,0,0,0,0,128)
        SPLT A 0 A_SpawnItemEx("DoomBlood",0,0,0,0,0,0,0,128)
        SPLT A 4 A_PlaySound("gibbage/xgibst")
        SPLT BCD 4
        SPLT D 70
        Stop        
    }
}

