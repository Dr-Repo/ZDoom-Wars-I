//Vanilla Flembomination
ACTOR Flembomination
{
  Health 4250
  Mass 6000
  Height 95
  Radius 78
  Speed 12
  Meleerange 275
  Damagefactor "Annihilate", 0
  Damagefactor "Ridiculous", 0.6
  Bloodcolor "Green"
  Bloodtype "GreenBlood"
  SeeSound "q3spider/sight"
  AttackSound "q3spider/attack"
  PainSound "q3spider/pain"
  DeathSound "q3spider/death"
  ActiveSound "q3spider/active"
  PainChance 0
  Painchance "ChexRemoverVanilla", 255
  Painchance "ChexTeleporterVanilla", 255
  Monster
  +FLOORCLIP
  +NORADIUSDMG
  +DONTMORPH
  +DONTRIP
  +NODROPOFF
  +NOICEDEATH
  +QUICKTORETALIATE
  +MISSILEMORE
  -DROPOFF
  -COUNTKILL
  -BOSSDEATH
  -BOSS
  Dropitem "RedMana", 256
  Dropitem "RedHeroMana", 256
  Dropitem "SlimeCanister", 256
  Obituary "%o was defeated by the Flembomination."
  States
  {
  Spawn:
	FBOM A 0
	FBOM A 0 A_RearrangePointers(AAPTR_DEFAULT,AAPTR_DEFAULT,AAPTR_TARGET)
	FBOM A 0 ACS_NamedExecuteAlways("HeroNotifications", 0, 30, 1)
	FBOM A 0 ACS_NamedExecuteAlways("HealthBar", 0, 125)
	FBOM AB 10 A_Look
	goto See
  See:
	FBOM A 0 A_JumpIfTargetinLOS(2, 180)
	FBOM A 0 A_Jump(25, "Giveup")
	FBOM A 1 A_PlaySound("gibbage/bslide")
	FBOM A 2 A_Chase
	FBOM ABB 3 A_Chase
	FBOM C 1 A_PlaySound("gibbage/bslide")
	FBOM C 2 A_Chase
	FBOM CDD 3 A_Chase
	loop
  Idle:
	FBOM A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	FBOM AB 10 A_Wander
	FBOM A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	loop
  Giveup:
	FBOM A 0 A_ClearTarget
	goto See
  Missile:
	FBOM A 18 A_FaceTarget  
	FBOM G 4 A_CustomMissile("FlembominationBall3", 48, 0, random(-3,4))
	FBOM G 0 A_Jump (240, 2)
	FBOM G 4 A_CustomMissile("FlemSporeSummonball",0,0,0)
	FBOM G 4 A_CustomMissile("FlembominationBall4", 48, 0, random(-4,3))
	FBOM H 4 A_CustomMissile("FlembominationBall3", 48, 0, random(-3,4))
	FBOM G 4 A_CustomMissile("FlembominationBall2", 48, 0, random(-4,3))
	FBOM G 4 A_CustomMissile("FlembominationBall4", 48, 0, random(-3,4))
	FBOM G 4 A_CustomMissile("FlembominationBall4", 48, 0, random(-4,3))
	FBOM H 4 A_CustomMissile("FlembominationBall3", 48, 0, random(-3,4))
	FBOM G 0 A_Jump (240, 2)
	FBOM G 4 A_CustomMissile("AFlemSporeSummonball",0,0,0)
	FBOM G 3 A_CustomMissile("FlembominationBall5", 48, 0, random(-4,3))
	FBOM G 4 A_CustomMissile("FlembominationBall4", 48, 0, random(-3,4))
	FBOM H 3 A_CustomMissile("FlembominationBall", 48, 0, random(-4,3))
	FBOM G 4 A_CustomMissile("FlembominationBall2", 48, 0, random(-3,4))
	FBOM H 4 A_CustomMissile("FlembominationBall4", 48, 0, random(-4,3))
	FBOM H 1 A_CPosRefire
	Goto Missile+1
  Melee:  
	FBOM H 8 A_FaceTarget
	FBOM G 8 A_CustomMeleeAttack (random(24, 30), "qdemon/melee")
	Goto see
  Pain:
	FBOM I 3
	FBOM I 3 A_Pain
	Goto see
  Death:
	FBOM A 0 ACS_NamedExecuteAlways("ManaReimbursement",0,13,1)
  	FBOM A 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	FBOM A 0 A_GiveToTarget("RedMana",1)
	FBOM A 0 A_GiveToTarget("RedHeroMana",1)
	FBOM A 0 ACS_NamedExecuteAlways("HeroNotifications", 0, 31, 1)
	FBOM J 20 A_Scream
	FBOM K 10 A_NoBlocking
	FBOM LLLLL 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
	FBOM L 1 A_CustomMissile ("GREENBOSSGIBBER", 16, 0, 120, 2, 12)
	FBOM LL 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
	FBOM MMMMMMMM 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
	FBOM NNNNNN 1 A_SpawnItemEx ("KaGas", 0, 0,0,random(-9,9),random(-9,9),random(5,9), 48, 128)
	FBOM N 1 A_CustomMissile ("GREENBOSSGIBBER", 37, 0, 48, 2, 64)
	FBOM OPQR 6
	FBOM S 300   
	stop
  Pain.ChexTeleporterVanilla:
	BSPI A 0 A_GiveInventory("TeleportCount",1)
	BSPI A 0 HealThing(1)
	BSPI A 0 A_JumpIfInventory("TeleportCount", ACS_ExecuteWithResult(118,0,4,0), "TeleportCooldown")
	BSPI A 0 Teleport(2024,0,0)
	goto TeleportCooldown
  TeleportCooldown:
	FBOM A 3 A_PlaySound("gibbage/bslide")
	FBOM ABB 3 A_Chase
	FBOM C 1 A_PlaySound("gibbage/bslide")
	FBOM C 2 A_Chase
	FBOM CDD 3 A_Chase
	goto See
  Pain.ChexRemoverVanilla:
	FBOM A 0 A_Die("ChexRemoverVanilla")
	Stop
  Death.ChexRemoverVanilla:
	FBOM A 0 A_UnsetSolid
	FBOM A 0 ACS_NamedExecuteAlways("HeroNotifications", 0, 59, 7)
	FBOM A 0 ACS_NamedExecuteAlways("ManaReimbursement",0,13,1)
	FBOM A 0 A_SpawnItemEx("ChexTelefog")
	FBOM A 0 A_SpawnItem ("RedMana")
	FBOM A 0 A_SpawnItem ("RedHeroMana")
	Stop
	}
}

