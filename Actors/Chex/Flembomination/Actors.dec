	FBOM G 3 A_CustomMissile("SFlembominationBall5", 48, 0, random(-4,3))
	FBOM G 4 A_CustomMissile("SFlembominationBall4", 48, 0, random(-3,4))
	FBOM H 3 A_CustomMissile("SFlembominationBall", 48, 0, random(-4,3))
	FBOM G 4 A_CustomMissile("SFlembominationBall2", 48, 0, random(-3,4))
	FBOM H 4 A_CustomMissile("SFlembominationBall4", 48, 0, random(-4,3))
	FBOM H 1 A_CPosRefire
	Goto Missile+1
  Pain.SilverTeamTeleporter:
	BSPI A 0 A_GiveInventory("TeleportCount",1)
	BSPI A 0 HealThing(1)
	BSPI A 0 A_JumpIfInventory("TeleportCount", ACS_ExecuteWithResult(118,0,4,0), "TeleportCooldown")
	BSPI A 0 Teleport(2039,0,0)
	goto TeleportCooldown
   }
