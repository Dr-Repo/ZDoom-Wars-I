//Vanilla Upgraded Armored Bipedal
ACTOR AArmoredBiped : ArmoredBiped
{
  states
  {
  Spawn:
    TABU AB 10 A_Look
    goto See
  See:
    TABU A 0 A_JumpIfTargetinLOS(2, 140)
    TABU A 0 A_Jump(20, "Giveup")
    TABU AAB 3 A_Chase
    TABU B 3 A_PlaySound("gibbage/sstep")
    TABU CCD 3 A_Chase
    TABU D 3 A_PlaySound("gibbage/sstep")
    loop
  Idle:
	TABU A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	TABU AABBCCDD 3 A_Wander
	TABU A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	loop
  Giveup:
    TABU A 0 A_ClearTarget
    goto See
  Missile:
	TABU E 1 A_Jump (20, "Missile2")
	TABU EF 5 A_FaceTarget
	TABU G 3 A_CustomMissile ("ArmoredBipedalBall", 32, 0, 0)
    goto See
  Missile2:
	TABU EF 3 A_FaceTarget
	TABU G 0 A_CustomMissile("aFlemsporeSummonball",0,0,0)
	goto Missile+3
  Pain:
    TABU H 2
    TABU H 2 A_Pain
    goto See
  Death:
  	ARFL I 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	TABU I 0 A_GiveToTarget("GreenMana",1)
	TABU I 0 A_GiveToTarget("GreenHeroMana",1)
    TABU I 8 
    TABU J 8 A_Scream
    TABU K 6
    TABU L 6 A_NoBlocking
	TABU M 0 A_JumpIfInventory("Revived", 1, "Death2")
    CHA2 A -1 
    stop
  XDeath:
  	ARFL I 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	TABU I 0 A_GiveToTarget("GreenMana",1)
	TABU I 0 A_GiveToTarget("GreenHeroMana",1)
    TABU N 1 A_SpawnitemEx ("GIB1GREEN", 0, 0, random(0,42), 0, 0, random(2,6), random(0,360), 128)
    TABU N 1 A_SpawnitemEx ("GIB2GREEN", 0, 0, random(0,42), 0, 0, random(2,6), random(0,360), 128)
