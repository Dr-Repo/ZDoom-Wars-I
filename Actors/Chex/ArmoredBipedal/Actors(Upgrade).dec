	TABU A 0 A_KillChildren
	TABU A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.DoomConvert2")
    TABU A 0 A_SpawnItemEx("DoomAArmoredBiped",0,0,0,0,0,0,0,SXF_SETMASTER)
