    TABU P 1 A_SpawnitemEx ("GIB5GREEN", 0, 0, random(0,42), 0, 0, random(2,6), random(0,360), 128)
    TABU P 2 
    TABU Q 5 A_NoBlocking
    TABU RST 5
	TABU U 0 //A_JumpIfInventory("Revived", 1, "Death2")
    TABU U -1
    stop
  Raise:
    TABU MLKJI 5
	TABU A 10
    goto See
  Death2:
	TNT1 A 0
    stop
  Pain.Revived:
