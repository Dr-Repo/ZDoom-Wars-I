//Vanilla Slime Carrier
ACTOR SlimeCarrier
{
  Health 600
  Radius 20
  Height 60
  Mass 250
  Speed 7
  PainChance 0
  Painchance "ChexRemoverVanilla", 255
  Painchance "ChexTeleporterVanilla", 255
  Bloodcolor "Green"
  Bloodtype "GreenBlood"
  Minmissilechance 20
  MONSTER
  -COUNTKILL
  +FLOAT 
  +PUSHABLE
  +DONTMORPH
  +DONTFALL
  +QUICKTORETALIATE
  +MISSILEMORE
  +MISSILEEVENMORE
  +LOOKALLAROUND
  Obituary "%o was killed by a balloon?!?!?"
  states
  {
  Spawn:
	SLCR D 0
	SLCR D 0 A_RearrangePointers(AAPTR_DEFAULT,AAPTR_DEFAULT,AAPTR_TARGET) // I can't believe it had to come to this. At a complete loss for words.
	goto Spawn2
  Spawn2:
	SLCR D 0 A_JumpIfInventory("CarrierReady",15, "Spawn3")
  	SLCR D 35 A_PlaySound("meteor")
	SLCR A 35 
	SLCR D 0 A_GiveInventory("CarrierReady",1)
	loop
  Spawn3:
	SLCR C 0 A_ChangeFlag("NoGravity", 1)
	SLCR C 1 ThrustThingZ(0, random(40, 100), 0, 0)
	SLCR CCCCCCC 1 A_PlaySound("slimebellows", CHAN_ITEM, 4.0, 1)
	SLCR C 1 A_Stop
	Goto See
  See:
    SPOS A 0 A_JumpIfTargetinLOS(2, 140)
    SPOS A 0 A_Jump(30, "Giveup")
    SLCR A 1 A_PlaySound("slimebellows", CHAN_ITEM, 4.0, 1) 
	SLCR A 3 A_Chase
  	SLCR CACACACACACACACACAC 4 A_Chase
  	loop
  Idle:
    SLCR A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	SLCR DA 35 A_Wander 
	SLCR A 0 A_LookEx(0, 0, 0, 0, 360, "See")
	loop
  Missile:
    SLCR A 0 A_JumpIfCloser(401, 1)
	goto See
    SLCR A 0 A_JumpIfHealthLower(500,9)
	SLCR B 10 A_PlaySound("slimebellows", CHAN_ITEM, 4.0, 1) 
  	SLCR B 0 A_CustomMissile("aFlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("aFlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("aFlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("aFlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("SlimePuddleSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("SlimePuddleSummonball",0,0,random(0,360))
    SLCR A 0 A_JumpIfHealthLower(400,9)
	SLCR C 10 A_PlaySound("slimebellows", CHAN_ITEM, 4.0, 1) 
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("SlimePuddleSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("SlimePuddleSummonball",0,0,random(0,360))
    SLCR A 0 A_JumpIfHealthLower(300,7)
	SLCR B 10 A_PlaySound("slimebellows", CHAN_ITEM, 4.0, 1) 
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR A 0 A_JumpIfHealthLower(200,8)
	SLCR C 10 A_PlaySound("slimebellows", CHAN_ITEM, 4.0, 1) 
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR A 0 A_JumpIfHealthLower(100,8)
	SLCR C 10 A_PlaySound("slimebellows", CHAN_ITEM, 4.0, 1) 
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR C 10 A_PlaySound("slimebellows", CHAN_ITEM, 4.0, 1) 
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
  	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	SLCR B 0 A_CustomMissile("FlemSporeSummonball",0,0,random(0,360))
	Goto Death
  Giveup:
    SLCR A 0 A_ClearTarget
    goto See
  Death:
    SLCR CBCB 3
    SLCR AAAAAA 0 A_SpawnItemEx("KaGas", 0,0,random(0,32), random(6,9),random(6,9),random(-3,5), random(0,360), 128)
    SLCR A 0 A_NoBlocking
    stop
  Pain.ChexTeleporterVanilla:
	SLCR A 0 A_GiveInventory("TeleportCount",1)
	SLCR A 0 HealThing(1)
	SLCR A 0 A_JumpIfInventory("TeleportCount", ACS_ExecuteWithResult(118,0,4,0), "See")
	SLCR A 0 Teleport(2024,0,0)
	goto Spawn
  Pain.ChexRemoverVanilla:
	SLCR A 0 A_Die("ChexRemoverVanilla")
	Stop
  Death.ChexRemoverVanilla:
	SLCR A 0 A_UnsetSolid
	SLCR A 0 A_SpawnItemEx("ChexTelefog")
	SLCR A 0
	Stop
  }
}

