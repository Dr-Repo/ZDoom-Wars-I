	frame YHAD { light YELLOWFGARG }
	frame YHEX { light YELLOWFGARG }
}

pointlight FLEMMINE
{
    color 0.0 1.0 0.0
    size 256
}

pointlight FLEMMINE_X1
{
    color 1.0 1.0 1.0
