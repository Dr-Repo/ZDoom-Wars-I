//Vanilla Spectre
ACTOR ZSpectre : ZDemon replaces spectre
{
  Health 225
  +GHOST
  RenderStyle Translucent
  Alpha 0.5
  Dropitem "GreenMana", 256
  Dropitem "GreenHeroMana", 256
  Dropitem "ZArmor", 32
  States
  {
  Melee:
    SARG EF 6 A_FaceTarget
    SARG G 8 A_CustomMeleeAttack(12, "demon/melee")
    Goto See
  Death:
  	SARG H 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	SARG H 0 A_GiveToTarget("GreenMana",1)
	SARG H 0 A_GiveToTarget("GreenHeroMana",1)
    SARG H 0 A_Jump (128, "Death2")
    SARG I 8
    SARG J 8 A_Scream
    SARG K 4
    SARG L 4 A_NoBlocking
    SARG M 4 A_PlaySound("gibbage/xbody")
	SARG N 1 A_JumpIfInventory("Revived", 1, "Death3")
    SARG N -1
    stop
   Death2:
	SAAR A 2 A_Scream
	SAAR A 0 A_SpawnItemEx("SPECTREARM",0, 0, 50, random(4,6), random(4,6), random(4,6), random(0,360), 128)
	SAAR A 3
	SAAR B 7
	SAAR C 6
	SAAR D 5 A_NoBlocking
	SAAR E 8
	SAAR F 1 A_JumpIfInventory("Revived", 1, "Death4")
	SAAR F -1
	stop
  XDeath:
  	TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	TNT1 A 0 A_GiveToTarget("GreenMana",1)
	TNT1 A 0 A_GiveToTarget("GreenHeroMana",1)
    TNT1 A 0 A_NoBlocking
    TNT1 A 1 A_PlaySound("gibbage/xsplat") 
    TNT1 A 1 A_SpawnItemEx("SPECTREHEAD",0, 0, 50, random(4,6), random(4,6), random(4,6), random(0,360), 128)
    TNT1 A 1 A_SpawnItem("GIBBER")
    TNT1 AA 1 A_SpawnItemEx("SPECTRELEG",0, 0, 50, random(4,6), random(4,6), random(4,6), random(0,360), 128)
    Stop
  Death.DOOMConvert:
    SARG A 0 A_UnsetSolid
	SARG A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.DoomConvert2")
    SARG A 0 A_SpawnItemEx("ZSpectre",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	SARG A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.DoomConvert2:
	SARG A 0 A_UnsetSolid
    SARG A 0 A_SpawnItemEx("ZSpectre")
	Stop
  Death.HereticConvert:
    SARG A 0 A_UnsetSolid
