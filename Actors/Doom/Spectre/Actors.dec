	TNT1 A 35
	SARG A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.HereticConvert2:
	SARG A 0 A_UnsetSolid
    SARG A 0 A_SpawnItemEx("HereticZSpectre")
	Stop
  Death.HexenConvert:
