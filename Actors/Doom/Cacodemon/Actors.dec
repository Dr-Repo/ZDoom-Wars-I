	TNT1 A 35
	HEAD A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.VirusConvert2:
	HEAD A 0 A_UnsetSolid
    HEAD A 0 A_SpawnItemEx("VirusZCacodemon")
	Stop
  Pain.BlueConvert:
	HEAD A 0 A_Die("BlueConvert")
