	HEAD A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.GreenConvert2")
    HEAD A 0 A_SpawnItemEx("GreenZCacodemon",0,0,0,0,0,0,0,SXF_SETMASTER)
