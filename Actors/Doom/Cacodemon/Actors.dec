  states
  {
  Spawn:
	HEAD A 0
	HEAD A 0 A_RearrangePointers(AAPTR_DEFAULT,AAPTR_DEFAULT,AAPTR_TARGET)
	HEAD A 0 ACS_NamedExecuteAlways("ManaReimbursement", 0,0,4)
	HEAD A 10 A_Look
    goto see
  See:
    HEAD A 0 A_JumpIfTargetinLOS(2, 140)
    HEAD A 0 A_Jump(20, "Giveup")
    HEAD A 1 A_PlaySound("gibbage/fly")
    HEAD AAAAAAAAAAAAAAA 3 A_Chase
    goto see
  Idle:
    HEAD AAAAAAA 3 A_Wander
	HEAD A 3 A_LookEx(0, 0, 0, 0, 360, "See")
	HEAD AAAAAAA 3 A_Wander
    loop
  Giveup:
    HEAD A 0 A_ClearTarget
    goto See
  Missile:
    HEAD BC 6 A_FaceTarget
    HEAD D 7 bright A_CustomComboAttack("Zcacodemonball", 26, random(13, 14), "caco/attack", Normal, 1)
    goto See
  Pain:
    HEAD E 3
    HEAD E 3 A_Pain
    HEAD F 4
    goto See
  Death:
  	HEAD G 0 A_JumpIf(ACS_ExecuteWithResult(130,10)==0, 3)
	HEAD G 0 A_GiveToTarget("PinkMana",1)
	HEAD G 0 A_GiveToTarget("PinkHeroMana",1)
    HEAD G 8 A_Scream
    HEAD H 6
    HEAD H 0 A_SpawnItem("BLUEGIBBER")
    HEAD H 2
    HEAD I 5 A_PlaySound("gibbage/xvile")
    HEAD I 3 A_SpawnItem("BLUEGIBBER")
    HEAD J 8 A_NoBlocking
	HEAD K 8
	HEAD L 0 A_JumpIfInventory("Revived", 1, "Death2")
    HEAD L -1 A_SetFloorClip
    stop
  Death2:
	HEAD L 3 A_FadeOut(0.1)
    loop
  Raise:
    HEAD L 8 A_UnSetFloorClip
    HEAD KJIHG 8 A_DeQueueCorpse
	HEAD A 10
    goto See
  Pain.Revived:
	HEAD A 0 A_GiveInventory("Revived", 1)
	goto See
  Pain.DOOMConvert:
	HEAD A 0 A_Die("DOOMConvert")
	Stop
  Death.DOOMConvert:
    HEAD A 0 A_UnsetSolid
	HEAD A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.DoomConvert2")
    HEAD A 0 A_SpawnItemEx("zcacodemon",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	HEAD A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.DoomConvert2:
	HEAD A 0 A_UnsetSolid
    HEAD A 0 A_SpawnItemEx("ZCacodemon")
	Stop
  Pain.HereticConvert:
	HEAD A 0 A_Die("HereticConvert")
	Stop
  Death.HereticConvert:
    HEAD A 0 A_UnsetSolid
	HEAD A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.HereticConvert2")
    HEAD A 0 A_SpawnItemEx("HereticZCacodemon",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	HEAD A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.HereticConvert2:
	HEAD A 0 A_UnsetSolid
    HEAD A 0 A_SpawnItemEx("HereticZCacodemon")
	Stop
  Pain.HexenConvert:
	HEAD A 0 A_Die("HexenConvert")
	Stop
  Death.HexenConvert:
    HEAD A 0 A_UnsetSolid
	HEAD A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.HexenConvert2")
    HEAD A 0 A_SpawnItemEx("HexenZCacodemon",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	HEAD A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.HexenConvert2:
	HEAD A 0 A_UnsetSolid
    HEAD A 0 A_SpawnItemEx("HexenZCacodemon")
	Stop
  Pain.StrifeConvert:
	HEAD A 0 A_Die("StrifeConvert")
	Stop
  Death.StrifeConvert:
    HEAD A 0 A_UnsetSolid
	HEAD A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.StrifeConvert2")
    HEAD A 0 A_SpawnItemEx("StrifeZCacodemon",0,0,0,0,0,0,0,SXF_SETMASTER)
