  Death.BlueConvert:
    HEAD A 0 A_UnsetSolid
	HEAD A 0 A_JumpIf(ACS_ExecuteWithResult(130,3)==1, "Death.BlueConvert2")
    HEAD A 0 A_SpawnItemEx("BlueZCacodemon",0,0,0,0,0,0,0,SXF_SETMASTER)
	TNT1 A 35
	HEAD A 0 A_DamageChildren(1, "Revived")
	Stop
  Death.BlueConvert2:
	HEAD A 0 A_UnsetSolid
    HEAD A 0 A_SpawnItemEx("BlueZCacodemon")
	Stop
  Pain.GreenConvert:
	HEAD A 0 A_Die("GreenConvert")
